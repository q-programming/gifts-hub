{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@core-services/*": [
        "app/core/services/*"
      ],
      "@services/*": [
        "app/main/services/*"
      ],
      "@model/*": [
        "app/model/*"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "baseUrl": "src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
